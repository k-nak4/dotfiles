alignment top_right
gap_y 50
own_window_transparent yes
own_window_argb_visual true
own_window_argb_value 155
background yes
border_width 1
cpu_avg_samples 4
default_color white
default_outline_color white
default_shade_color white
draw_borders no
draw_graph_borders yes
draw_outline no
draw_shades no
override_utf8_locale yes
use_xft yes
xftfont TakaoGothic:style=Regular:size=12
minimum_size 320 5
maximum_width 320
net_avg_samples 2
no_buffers yes
out_to_console no
out_to_stderr no
extra_newline no
own_window yes
own_window_class Conky
own_window_type normal
own_window_hints undecorated,below,sticky,skip_taskbar,skip_pager
stippled_borders 0
update_interval 1.0
uppercase no
use_spacer none
show_graph_scale no
show_graph_range no
double_buffer yes

TEXT
${font Sans:size=18}$alignc${time %Y / %m / %d / %a}${font}
${font Ubuntu:size=36}$alignc${time %H:%M:%S}${font}
$hr
${color grey}Uptime:$color $uptime
${color grey}Frequency (in MHz):$color $freq
${color grey}RAM Usage:$color $mem/$memmax - $memperc%
$alignc${membar 5,300}
${color grey}Swap Usage:$color $swap/$swapmax - $swapperc%
$alignc${swapbar 5,300}
${color grey}CPU Usage:${color} ${cpu} %
# $alignc${cpubar 5,300}
$alignc${color black}${cpugraph cpu0 32,300 0000ff ff0000}
${color grey}CPU1 ${cpu cpu1} %
$alignc${cpubar cpu1 5,300}
${color grey}CPU2 ${cpu cpu2} %
$alignc${cpubar cpu2 5,300}
${color grey}CPU3 ${cpu cpu3} %
$alignc${cpubar cpu3 5,300}
${color grey}CPU4 ${cpu cpu4} %
$alignc${cpubar cpu4 5,300}
${color grey}Processes:$color $processes  ${color grey}Running:$color $running_processes
$hr
${color grey}Temperature:$color
 ${color grey}CPU$color ${alignr}${hwmon temp 1}℃
 ${color grey}M/B$color ${alignr}${hwmon temp 2}℃

 ${color grey}/dev/sda $color $alignr${hddtemp /dev/sda}°C
$hr
 ${color grey}Right Fan$color ${alignr}${execi 10 sensors | grep 'Right' | cut -c14-23}
$hr
${color grey}File systems:
 / $color${fs_used /}/${fs_size /}
$alignc${fs_bar 5,300 /}
$hr
${color grey}Networking:
 Down:${color #8844ee} ${downspeedf wlp3s0} k/s${color lightgrey} ${offset 50}Up:${color #22ccff} ${upspeedf wlp3s0} k/s
${color black}${downspeedgraph wlp3s0 32,150 ff0000 0000ff} $alignr${color black}${upspeedgraph wlp3s0 32,150 0000ff ff0000}
${color grey}Name${alignr} PID      CPU%   MEM%
${color lightgrey} ${top name 1} ${alignr}${top pid 1}     ${top cpu 1}     ${top mem 1}
${color lightgrey} ${top name 2} ${alignr}${top pid 2}     ${top cpu 2}     ${top mem 2}
${color lightgrey} ${top name 3} ${alignr}${top pid 3}     ${top cpu 3}     ${top mem 3}
${color lightgrey} ${top name 4} ${alignr}${top pid 4}     ${top cpu 4}     ${top mem 4}
